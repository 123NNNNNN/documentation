{
  "ci": {
    "collect": {
      "settings": {
        "output": ["html"],
        "skipAudits": ["is-cralwable"],
        "useThrottling": "true",
        "onlyCategories": [
            "performance",
            "accessibility",
            "best-practices",
            "seo"
        ]
    },
    "numberOfRuns": 3
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "offscreen-images": "off",
        "uses-webp-images": "off"
      }
    },
    "upload": {
      "target": "process.env.ARTIFACTS_FULL_DIR"
    }
  }
}